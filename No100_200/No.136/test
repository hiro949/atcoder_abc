//Eucledian algorithm
int gcd_two ( int a, int b ){
    int m,n;
    if( abs(a)>abs(b) ){
         m=abs(a);n=abs(b);
    }else{
         m=abs(b);n=abs(a); 
    }
    if( n == 0 ) return m;
    return gcd_two( n, m%n );
}

int gcd( vector<int> &v ){
    int size = v.size();
    if( size==1 ) return abs(v[0]);
    else if( size==2 ) return gcd_two ( v[0], v[1] );
    vector<int> vl, vr;
    copy(v.begin(), v.begin()+size/2, vl.begin());
    copy(v.begin()+size/2, v.end(), vr.begin());
    return gcd_two ( gcd( vl ), gcd( vr ) );
}